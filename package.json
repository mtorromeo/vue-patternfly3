    "postcss": "^8.2.10",
    "postcss-loader": "^3.0.0",
    "resize-observer-polyfill": "^1.5.0",
    "rollup": "^2.15.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-vue": "^5.0.0",
